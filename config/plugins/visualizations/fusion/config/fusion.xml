<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE visualization SYSTEM "../../visualization.dtd">
<visualization name="fusion_inspector">
          <data_sources>
               <data_source>
                      <model_class>HistoryDatasetAssociation</model_class>
                       <test type="isinstance" test_attr="datatype" result_type="datatype">
			 text.Json
		       </test>
                       <test test_attr="creating_job.tool_id">
			 toolshed.g2.bx.psu.edu/repos/bhaas/fusion-inspector-new/0.0.3
                       </test>
                       <to_param param_attr="id">dataset_id</to_param>
                </data_source>
           </data_sources>
           <params>
                     <param type="dataset" var_name_in_template="hda" required="true">dataset_id</param>
           </params>
           <template>fusion_inspector.mako</template>
</visualization>
